{
  "name": "dark-mode",
  "displayName": "Dark mode",
  "version": "0.0.1",
  "description": "change website dark or light mode",
  "author": "Shiyue (openai0529@gmail.com)",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.464.0",
    "plasmo": "0.89.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "prettier": "3.2.4",
    "tailwindcss": "^3.4.16",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "<all_urls>"
    ],
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0.0",
    "permissions": [
      "storage"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "ts": [
          "content.ts"
        ]
      }
    ],
    "background": {
      "service_worker": "service-worker.ts"
    }
  }
}
